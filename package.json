{
  "name": "ng-di-kit",
  "version": "0.0.3",
  "description": "Angular Dependency Injection Kit",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "clean": "rm -rf lib/*",
    "build": "npm run clean && tsc",
    "patch": "npm run build && npm run test && npm version patch -m \"[PATCH] version (%s)\" && npm publish",
    "test": "./node_modules/.bin/jasmine-ts src/specification/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joejukan/ng-di-kit.git"
  },
  "keywords": [
    "Angular",
    "Dependency",
    "Injection",
    "DI"
  ],
  "author": "Joseph Eniojukan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joejukan/ng-di-kit/issues"
  },
  "homepage": "https://github.com/joejukan/ng-di-kit#readme",
  "dependencies": {
    "@angular/core": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "rxjs": "^5.5.0",
    "zone.js": "^0.8.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/underscore": "^1.8.8",
    "jasmine-ts": "^0.2.1",
    "jasmine-spec-reporter": "^4.2.1"
  }
}
